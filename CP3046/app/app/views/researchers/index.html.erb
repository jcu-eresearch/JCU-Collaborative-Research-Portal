<h1>Listing Researchers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th></th>
  </tr>

<%= will_paginate @researchers %>
<% @researchers.each do |researcher| %>
  <tr>
    <td><%= researcher.name %></td>
    <td><%= researcher.title %></td>
    <td><%= link_to 'Profile', researcher %></td>
  </tr>
<% end %>
</table>

<br />
